<!--miniprogram/pages/index/heatmap.wxml-->
<cu-custom bgColor="bg-gradual-orange" isBack>
    <view slot="content">热力图</view>
</cu-custom>
<view class="bg-gradual-blue shadow shadow-lg">
    <canvas style="width: {{width}}px; height: {{height}}px;" canvas-id="firstCanvas" bindtap="StopRun"></canvas>
    <canvas style="width: 256px; height: 5px; position: fixed;bottom:5px; left:5px;" canvas-id="PaletteCanvas"></canvas>
</view>
